<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>afirma-handwritten-storage-service</display-name>
  <welcome-file-list>
    <welcome-file>handwritten_demo.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description>Servicio de almacenamiento de firmas</description>
    <display-name>StorageService</display-name>
    <servlet-name>StorageService</servlet-name>
    <servlet-class>es.gob.afirma.handwritten.services.StorageService</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StorageService</servlet-name>
    <url-pattern>/StorageService</url-pattern>
  </servlet-mapping>
</web-app>